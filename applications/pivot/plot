set term pos eps enh mono 12
set size 0.7,0.7
unset title
set xrange [600:610]
set yrange [0:0.05]
set xlabel 'Time (min)'
set ylabel 'Velocity (m/s)'
set out 'pivot-velocity.eps'
plot 'dyn-25' u ($1/60.):4 t'T1' w l lt 4,\
	'' u ($1/60.):7 t'T2' w l lt 3,\
	'' u ($1/60.):10 t'T3' w l lt 2,\
	'' u ($1/60.):13 t'T4' w l lt 1
