	'' u 4 ti col w histogram fill empty border 1 lt 1 lw 0.25, \
	'' u 6 ti col w histogram fill solid 0.5 border 1 lt 1 lw 0.25, \
	'' u 8 ti col w histogram fill pattern 4 border 1 lt 1 lw 0.25

set ylabel 'Number of stops'
set xrange [-0.5:18.5]

set title 'Simulated'
